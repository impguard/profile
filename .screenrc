escape ^]a

bell_msg '^GBell in window %n'

width 80
height 24
defscrollback 1000000

shell -/bin/bash

screen -t editor
screen -t build
screen -t admin
screen -t services
screen -t top
screen -t services2
screen -t local
screen -t remote
select 1
select 0

startup_message off
hardstatus alwayslastline
hardstatus string '%{gk}[ %{G}%H %{g}][%= %{wk}%?%-Lw%?%{=b kR} (%{W}%n*%f %t%?(%u)%?%{=b kR})%{= kw}%?%+Lw%?%?%= %{g}][%{Y}%l%{g}] %{=b C}[ %m/%d %c ]%{W}'

attrcolor b ".I"
termcapinfo xterm 'Co#256:AB=\E[48;5;%dm:AF=\E[38;5;%dm'
defbce "on"
# set TERM
term xterm-256color
vbell on
vbell_msg ''
termcapinfo *  vb=:
